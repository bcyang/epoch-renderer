{
    "manifest_version": 3,
    "name": "Epoch Converter",
    "version": "1.0",
    "description": "Converts epoch timestamps to human-readable dates.",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}